{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "typescript": {} // this loads <rootdir>/tsconfig.json to eslint
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "object-shorthand": "warn",
    "prettier/prettier": "off"
  },
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {}
    }
  ]
}
